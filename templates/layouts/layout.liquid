<!doctype html>
<html lang="en">
<head>
  <!-- Title block allowing child pages to override -->
  <title>{% block title %}Anthem on Ashley-Old Fourth Ward Apartments{% endblock %}</title>

  <link rel="shortcut icon" type="image/ico" href="/favicon.ico" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
  <link href='http://fonts.googleapis.com/css?family=Montserrat:700' rel='stylesheet' type='text/css'>

  <!-- Device Specific Metadata -->
  <meta name="viewport" content="width=device-width">
  <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no">
  
  <!-- Vendor Styles -->
  

  <!-- Global Styles -->
  {{ "main.css" | asset_url | stylesheet_tag }}

  <!-- Style block allowing child pages to inject their own styles -->
  {% block styles %}{% endblock %}
</head>
<body>

  <div id="wrapper">
    <!-- Content block, child page inject content -->
    {% block content %}{% endblock %}
  </div>

  <!-- Vendor Scripts -->
  {{ "jquery.min.js" | asset_url | script_tag }}
  {{ "bootstrap.js" | asset_url | script_tag }}

  <!-- Google Map -->
  {{ "google-map.js" | asset_url | script_tag }}

  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBHjfEAx3kun_6_SvIHIdSl04ayMBuRRi0&callback=initMap">
    </script>

  <!-- Script block allowing child pages to inject their own scripts -->
  {% block scripts %}{% endblock %}

  <!-- Google Analytics -->
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-7543288-49', 'auto');
    ga('send', 'pageview');

  </script>
  
</body>
</html>